{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(filename)s - %(funcName)s - %(lineno)d - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "stromLogger": {
      "level": "DEBUG",
      "handlers": [
        "console"
      ],
      "propagate": "no"
    },
    "root": {
      "level": "DEBUG",
      "handlers": ["console"]
    }
  }
}